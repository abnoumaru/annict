{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["plugin:import/errors", "plugin:import/warnings", "eslint:recommended"],
  "globals": {
    "AnnConfig": true,
    "ga": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "import/no-default-export": "error"
  }
}
